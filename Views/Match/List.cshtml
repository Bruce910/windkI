@model IEnumerable<Final10._14.Models.TMatch>

@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FHelpId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FMemberId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FMatchDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FPoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FMatchStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FGrade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FGradeDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FMessage)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FHelpId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FMemberId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FMatchDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FPoint)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FMatchStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FGrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FGradeDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FMessage)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FMatchId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FMatchId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FMatchId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

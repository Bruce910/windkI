@model Final10._14.Models.TSponsor

@{
    ViewData["Title"] = "Details";
}

<h1>內容</h1>

<div>
    <h4>贊助商</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            贊助商名稱
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FSponsorName)
        </dd>
        <dt class = "col-sm-2">
            贊助商類別
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FSponsorCategoryId)
        </dd>
        <dt class = "col-sm-2">
            贊助商地址
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FAddress)
        </dd>
        <dt class = "col-sm-2">
            贊助商電話
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FPhone)
        </dd>
        <dt class = "col-sm-2">
            資本額
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FCapital)
        </dd>
        <dt class = "col-sm-2">
            介紹
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FIntroduction)
        </dd>
        <dt class = "col-sm-2">
            合作狀態
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FStatus)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.FSponsorId">編輯</a> |
    <a asp-action="Index">回列表</a>
</div>
